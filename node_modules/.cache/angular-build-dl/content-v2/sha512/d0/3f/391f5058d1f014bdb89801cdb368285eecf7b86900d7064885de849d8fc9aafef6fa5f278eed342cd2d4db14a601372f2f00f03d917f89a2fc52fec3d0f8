class AppService {
    constructor() {
        this.items = new rxjs__WEBPACK_IMPORTED_MODULE_1__["BehaviorSubject"]({ id: 1, name: 'todolist', description: 'This is todo list app' });
        this.totalItems = [{ id: 1, name: 'todolist', description: 'This is todo list app' }];
        this.items.next(this.totalItems[0]);
    }
    setDeletedItems(deletedItem) {
        console.log('deleting item::::' + deletedItem);
        this.totalItems = this.totalItems.filter(item => item.id !== deletedItem.id);
    }
    addItems(item) {
        const totalItemsLength = this.totalItems.length;
        item.id = totalItemsLength + 1;
        this.totalItems.push(item);
        this.items.next(item);
    }
    get ttlItems() {
        return this.totalItems;
    }
}
